{
  "$schema": "https://raw.githubusercontent.com/stryker-mutator/stryker-mutator/master/packages/api/schema/stryker-config.json",
  "mutate": [
    "src/**/*.ts",
    "!src/**/*.spec.ts",
    "!src/**/*.test.ts",
    "!src/**/*.mock.ts",
    "!src/main.ts",
    "!src/environments/*.ts",
    "!src/app/app.routes.ts",
    "!src/app/features/attribute-card/attribute-card.component.ts"
  ],
  "testRunner": "karma",
  "karma": {
    "projectType": "angular-cli",
    "configFile": "config/karma.conf.js",
    "config": {
      "browsers": ["ChromeHeadlessCI"]
    }
  },
  "reporters": ["progress", "clear-text", "html", "json"],
  "checkers": ["typescript"],
  "tsconfigFile": "tsconfig.stryker.json",
  "concurrency": 1,
  "timeoutMS": 120000,
  "thresholds": {
    "high": 90,
    "low": 85,
    "break": 90
  },
  "tempDirName": "stryker-tmp"
}
